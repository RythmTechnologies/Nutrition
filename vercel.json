{
  "version": 2,
  "builds": [
    {
      "src": "dietetic/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "https://rythmtech-nutrition-bucket.s3.amazonaws.com/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dietetic/wsgi.py"
    }
  ]
}
